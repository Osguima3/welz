{
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable -A -",
    "manifest": "deno task cli manifest $(pwd)",
    "migrate": "deno run --env-file --allow-read --allow-env --allow-net --import-map=db/import_map.json https://deno.land/x/nessie/cli.ts migrate -c ./db/nessie.config.ts",
    "seed": "deno run --env-file --allow-read --allow-env --allow-net --import-map=db/import_map.json https://deno.land/x/nessie/cli.ts seed -c ./db/nessie.config.ts",
    "start": "deno run --env-file --allow-read --allow-env --allow-net --watch=src/ src/main.ts",
    "build": "deno run --env-file --allow-read --allow-env --allow-net src/main.ts build"
  },
  "compilerOptions": {
    "lib": ["deno.ns", "deno.unstable"]
  }
}
